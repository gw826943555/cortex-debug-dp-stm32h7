{
	"publisher": "jeandudey",
	"name": "cortex-debug-dp-stm32h7",
	"displayName": "Cortex-Debug: Device Support Pack - STM32H7",
	"description": "This package contains additional support functionality (SVD definitions) for Cortex-Debug for STMicroelectronics STM32H7 devices",
	"version": "1.9.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/jeandudey/cortex-debug-dp-stm32h7.git"
	},
	"license": "Apache-2.0",
	"homepage": "https://github.com/jeandudey/cortex-debug-dp-stm32h7",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Debuggers"
	],
	"keywords": [
		"Cortex-Debug",
		"Cortex-Debug Device Support Pack",
		"cortex-m",
		"gdb",
		"debug",
		"embedded"
	],
	"activationEvents": [
		"onDebugResolve:cortex-debug"
	],
	"main": "./out/extension.js",
	"contributes": {},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
